# Copy this file to .env and fill in the values. Do NOT commit .env with secrets.

# Mockaroo API key (optional - used to fetch sample data)
MOCKAROO_API_KEY=

# GitHub personal access token with repo permissions
GITHUB_TOKEN=
# Alternatively GITHUB_PAT can be used
GITHUB_PAT=

# Repository to upload datasets to. Prefer owner/repo format.
GITHUB_REPO=AsifaSiraj/DWM-Project
# Optional owner if you prefer to set only repo name
GITHUB_REPO_OWNER=

# Local folder in the repo where datasets are stored
DATASET_FOLDER=Database/Datasets
